package ifElseStatements;

public class ifElseClass1 {

	public static void main(String[] args) {
		int a=20;
		int b=10;
		
		if(a>b)
		{
			System.out.println("a is greater than b");
	
		}else if (a==b)
			System.out.println("b is equal to a");
	 {
		System.out.println("b is greater than a");
		
	}
	}

}
